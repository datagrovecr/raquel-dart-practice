*Do-While Loops*

void main() {
  int beers = 10;
  
  do {
    print("$beers bottles of beer on the wall, $beers bottles of beer.");
    print("You take one down, pass it around... ");
    if(beers == 2){
      print("${beers - 1} bottle of beer on the wall.\n");
    }else{
      print("${beers - 1} bottles of beer on the wall.\n");
    }
    beers--;
  } while (beers > 0);
}
