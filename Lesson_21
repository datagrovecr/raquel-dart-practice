*Variable Scope*

int age = 15;

void main(){
  bool adult;
  int age = 30; //se utiliza esta variable de age
  
  if (age >= 21) {
    adult = true;
  }else {
    adult = false;
  }
  
  printAgeCategory(adult);
}

void printAgeCategory(bool isAdult){
  if(isAdult){
    print("Adult, age $age.");
  }else {
    print("Minir, age $age.");
  }
}
