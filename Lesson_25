*More About Lists*

void main() {
  List<String> students = ["Mary","Jon", "Sally"];

  String userInput = "Bob";

  students.add(userInput);

  print(students);

  students.remove("Jon");
  print(students);
  
  students.sort();
  print(students);
  
  String first = students.first;
  String last = students.last;
  print("The firs is $first and the last $last");

  printList(students);
  
  printList(["Dart", "JavaScript", "TypeScript", "C++"]);

}

void printList(List<String> list) {
    //for (int i = 0; i < list.length; i++) {
    //print("$i: ${list[i]}");
    
    for (String element in list) {
    print(element);
      
  }
}
